	<title>{{ CURRENT_PAGE.title }}</title>
	<link rel="stylesheet" href="{{ pathToRoot }}css/style.css">
	<meta name="HandheldFriendly" content="True">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<link rel="canonical" href="/{{ page }}.html">
